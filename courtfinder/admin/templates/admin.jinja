{% extends "base.jinja" %}
{% load staticfiles %}

{% block before_content %}
<div class="nav-menu">
  <ul>
      <li><a href="{% url 'admin:courts' %}">courts</a></li>
      <li><a href="{% url 'admin:users' %}">users</a></li>
      <li class="right-link"><a href="{% url 'admin:logout' %}">log out</a></li>
  </ul>
</div>

{% if messages %}
<ul class="messages">
    {% for message in messages %}
    <li{% if message.tags %} class="{{ message.tags }}"{% endif %}>{{ message }}</li>
    {% endfor %}
</ul>
{% endif %}
{% endblock %}


{% block javascripts %}
<script src="{% static 'javascripts/vendor/tinymce/tinymce.min.js' %}" type="text/javascript"></script>
<script src="{% static 'javascripts/vendor/jquery-ui-1.12.1/jquery-ui.min.js' %}" type="text/javascript"></script>
<script src="{% static 'javascripts/admin.js' %}" type="text/javascript"></script>
{% endblock %}